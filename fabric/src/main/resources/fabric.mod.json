{
  "schemaVersion": 1,
  "name": "${mod_name}",
  "id": "${mod_id}",
  "version":  "${mod_version}",
  "description": "${mod_description}",
  "authors": ["${author}"],
  "contact": {"repo": "${repository}"},
  "license": "${license}",
  "environment": "*",
  "mixins": [
    "mod-common.mixins.json"
  ],
  "entrypoints": {
    "main": [
      "com.your_name.mod.your_mod_id.fabric.FabricCommon"
    ],
    "client": [
      "com.your_name.mod.your_mod_id.fabric.FabricClient"
    ]
  },
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": "1.20.1",
    "cobblemon": ">=1.5.0"
  }
}
